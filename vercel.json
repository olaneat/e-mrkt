{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(?!((index\\.html)|(assets/.*)|(favicon\\.ico)|(robots\\.txt)|(.*\\..+)))(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\..+)",
      "dest": "/$1"
    }
  ]
}